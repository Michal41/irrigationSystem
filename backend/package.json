{
  "name": "backend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "validate": "npm-run-all -s tscValidate format:check test",
    "tscValidate": "tsc --noEmit",
    "test": "dotenv -e .env.test vitest run",
    "start": "NODE_ENV=production node dist/src/app.js",
    "dev": "NODE_ENV=development nodemon --exec ts-node src/app.ts",
    "format": "prettier --write . --config .prettierrc",
    "format:check": "prettier --check \"**/*.{ts,tsx,md}\"",
    "migrate": "prisma migrate deploy",
    "migrate:test": "dotenv -e .env.test prisma migrate deploy"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/express": "^4.17.1",
    "@types/supertest": "^6.0.2",
    "dotenv-cli": "^7.4.2",
    "nodemon": "^3.0.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.8",
    "prisma": "^5.16.1",
    "supertest": "^7.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2",
    "vitest": "^2.0.2"
  },
  "dependencies": {
    "@google-cloud/local-auth": "2.1.0",
    "@google-cloud/pubsub": "^4.5.0",
    "@prisma/client": "5.16.1",
    "body-parser": "^1.20.2",
    "dotenv": "^16.3.1",
    "express": "^4.17.1"
  }
}
